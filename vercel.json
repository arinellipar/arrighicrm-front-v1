{
  "buildCommand": "pnpm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "devCommand": "pnpm run dev",
  "regions": ["gru1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "/api/proxy"
  },
  "functions": {
    "app/api/cep/[cep]/route.ts": {
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_PUBLIC_API_URL": "/api/proxy"
    }
  },
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "https://arrighi-bk-test-c2bth2axeacqasc3.brazilsouth-01.azurewebsites.net/api/:path*"
    }
  ]
}
